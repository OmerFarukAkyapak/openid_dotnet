{
  "App": {
    "Name": "OpenIDApi",
    "SelfUrl": "https://localhost:7085"
  },
  "OpenIdServer": {
    "Authority": "https://localhost:7264/",
    "Audience": "OpenID",
    "RequireHttpsMetadata": "false",
    "SigningKey": "<your-key>"
  },
  "ConnectionStrings": {
    "DefaultConnection": "<your-conn>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
